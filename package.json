{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "testing",
  "title": "Are.na",
  "description": "Browse arena using Raycast",
  "icon": "command-icon.png",
  "author": "isabelsa",
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "getChannelContents",
      "title": "Browse channel",
      "description": "Browsing a hardcoded are.na channel",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Are.na Access Token.",
      "link": "https://developer.raindrop.io/v1/authentication/token",
      "placeholder": "Enter your Are.na personal token."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "are.na": "^0.1.5",
    "node-fetch": "^3.3.0",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
